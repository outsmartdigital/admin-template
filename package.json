{
  "name": "template-web-spa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:dev": "NODE_ENV=development && env-cmd -f .env.development yarn build",
    "build:staging": "NODE_ENV=staging && env-cmd -f .env.staging yarn build",
    "build:prod": "NODE_ENV=production && env-cmd -f .env.production yarn build",
    "start": "yarn start:dev",
    "start:dev": "env-cmd -f .env.development next dev",
    "start:staging": "env-cmd -f .env.staging next start",
    "start:prod": "env-cmd -f .env.production next start",
    "get-schema": "graphql get-schema",
    "sync": "yarn sync:dev",
    "sync:dev": "NODE_ENV=development env-cmd -f .env.development yarn get-schema",
    "sync:staging": "dotenv -e .env.staging yarn get-schema",
    "sync:prod": "dotenv -e .env.production yarn get-schema",
    "sync:local": "dotenv -e .env.production yarn get-schema",
    "codegen": "apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --outputFlat --globalTypesFile=src/services/GlobalTypes.ts src/services/types",
    "deploy:dev": "yarn run build:dev && yarn run s3-website deploy --config-file .s3-website.development.json",
    "deploy:staging": "yarn run build:staging && yarn run s3-website deploy --config-file .s3-website.staging.json",
    "deploy:prod": "yarn run build:prod && yarn run s3-website deploy --config-file .s3-website.production.json",
    "style": "prettier --write \"./{src,pages}/**/*.{js,css,json,md,ts,tsx}\"",
    "lint": "eslint --fix \"./{src,pages}/**/*.{js,css,json,md,ts,tsx}\"",
    "v": "npm version -m \"v%s [skip ci]\""
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.43",
    "@types/react-select": "^3.0.12",
    "@types/react-table": "^7.0.16",
    "apollo": "2.22.0",
    "apollo-boost": "^0.4.7",
    "apollo-link-error": "^1.1.12",
    "babel-preset-env": "^1.7.0",
    "detect-browser-language": "^0.0.2",
    "dotenv-cli": "^3.1.0",
    "env-cmd": "^10.0.1",
    "graphql": "14.6.0",
    "graphql-cli": "^3.0.14",
    "graphql-tag": "^2.10.3",
    "history": "4.7.2",
    "immutable": "^4.0.0-rc.12",
    "js-sha256": "^0.9.0",
    "lint-staged": "^10.2.2",
    "monaco-editor": "^0.19.3",
    "next": "9.5.2",
    "re-resizable": "^6.2.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.2.0",
    "react-hot-loader": "^4.8.4",
    "react-intl": "^5.8.0",
    "react-monaco-editor": "^0.33.0",
    "react-number-format": "^4.2.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0",
    "react-simple-hooks-forms": "^0.2.11",
    "react-table": "^7.0.4",
    "reactn": "^2.2.7",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.7",
    "typeface-roboto-mono": "^0.0.75",
    "validate.js": "^0.13.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@svgr/webpack": "4.1.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.7.3",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^4.3.2",
    "@types/styled-components": "^4.1.14",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.1",
