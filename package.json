{
  "name": "template-web-spa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn start:ssr",
    "start:ssr": "IS_LOCAL=true node server.js",
    "start:ssg": "yarn start:dev",
    "start:dev": "next dev",
    "get-schema": "graphql get-schema",
    "sync": "yarn sync:dev",
    "sync:dev": "NODE_ENV=development env-cmd -f .env.development yarn get-schema",
    "sync:staging": "dotenv -e .env.staging yarn get-schema",
    "sync:prod": "dotenv -e .env.production yarn get-schema",
    "sync:local": "dotenv -e .env.production yarn get-schema",
    "codegen": "apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --outputFlat --globalTypesFile=src/services/GlobalTypes.ts src/services/types",
    "build": "next build",
    "build:dev": "env-cmd -f .env.dev yarn build",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "build:prod": "env-cmd -f .env.prod yarn build",
    "export:ssg": "IS_LOCAL=true yarn next export",
    "deploy:dev:ssg": "IS_LOCAL=true yarn build:dev && yarn export:ssg && yarn run s3-website deploy --config-file .s3-website.development.json",
    "deploy:staging:ssg": "IS_LOCAL=true yarn build:staging && yarn export:ssg && yarn run s3-website deploy --config-file .s3-website.staging.json",
    "deploy:prod:ssg": "IS_LOCAL=true yarn build:prod && yarn export:ssg && yarn run s3-website deploy --config-file .s3-website.production.json",
    "deploy:dev:ssr": "yarn config-build && STAGE=dev yarn build:dev && sls s3sync -s dev --env dev && sls deploy -s dev --env dev",
    "deploy:staging:ssr": "yarn config-build && STAGE=staging yarn build:staging && sls s3sync -s staging --env staging && sls deploy -s staging --env staging",
    "deploy:prod:ssr": "yarn config-build && STAGE=prod yarn build:prod && sls s3sync -s prod --env prod && sls deploy -s prod --env prod",
    "logs:dev:ssr": "yarn sls logs -s dev -f webApp -t",
    "logs:staging:ssr": "yarn sls logs -s staging -f webApp -t",
    "logs:prod:ssr": "yarn sls logs -s prod -f webApp -t",
    "config-build": "node scripts/setBuildId.js",
    "style": "prettier --write \"./{src,pages}/**/*.{js,css,json,md,ts,tsx}\"",
    "lint": "eslint --fix \"./{src,pages}/**/*.{js,css,json,md,ts,tsx}\"",
    "v": "npm version -m \"v%s [skip ci]\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "serverless-http": "^2.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.9.6",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.43",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.7.3",
    "@types/react": "^16.8.14",
    "@types/styled-components": "^4.1.14",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "apollo": "^2.22.0",
    "apollo-boost": "^0.4.7",
    "apollo-link-error": "^1.1.12",
    "aws-serverless-express": "^3.3.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "detect-browser-language": "^0.0.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "4.2.0",
    "dotenv-webpack": "4.0.0",
    "env-cmd": "^10.0.1",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "fs-extra": "7.0.1",
    "graphql": "^14.6.0",
    "graphql-cli": "3.0.14",
    "graphql-tag": "^2.10.3",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^4.2.5",
    "lighthouse": "^5.2.0",
    "lint-staged": "^10.4.2",
    "next": "^9.5.2",
    "node-fetch": "^2.6.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.3",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.14.0",
    "react-intl": "^5.8.0",
    "react-simple-hooks-forms": "^0.2.11",
    "reactn": "^2.2.7",
    "resolve": "1.10.0",
    "s3-website": "^3.3.0",
    "serverless": "^2.4.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-layers": "^2.3.0",
    "serverless-s3-sync": "^1.15.0",
    "style-loader": "0.23.1",
    "styled-components": "^4.1.3",
    "ts-loader": "^7.0.3",
    "typedi": "^0.8.0",
    "typescript": "^3.8.3",
    "url-loader": "1.1.2",
    "validate.js": "^0.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,css,json,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "graphql": "14.6.0"
  }
}
